<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/destinationVideos">

    <fragment
        android:id="@+id/destinationVideos"
        android:name="lt.thumbnaildownloader.fragments.VideoListFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_video_list">
        <action
            android:id="@+id/action_destinationVideos_to_imagePreviewFragment"
            app:destination="@id/imagePreviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/imagePreviewFragment"
        android:name="lt.thumbnaildownloader.fragments.ImagePreviewFragment"
        android:label="fragment_image_preview"
        tools:layout="@layout/fragment_image_preview" />
</navigation>
